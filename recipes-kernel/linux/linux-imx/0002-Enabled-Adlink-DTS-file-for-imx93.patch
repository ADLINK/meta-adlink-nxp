 arch/arm64/boot/dts/adlink/Makefile | 7 +++++++
 2 files changed, 8 insertions(+)
 create mode 100644 arch/arm64/boot/dts/adlink/Makefile

diff --git a/arch/arm64/boot/dts/Makefile b/arch/arm64/boot/dts/Makefile
index 7b107fa7414b..f55d710fae2a 100644
--- a/arch/arm64/boot/dts/Makefile
+++ b/arch/arm64/boot/dts/Makefile
@@ -32,3 +32,4 @@ subdir-y += tesla
 subdir-y += ti
 subdir-y += toshiba
 subdir-y += xilinx
+subdir-y += adlink
diff --git a/arch/arm64/boot/dts/adlink/Makefile b/arch/arm64/boot/dts/adlink/Makefile
new file mode 100644
index 000000000000..68940b30e5a2
--- /dev/null
+++ b/arch/arm64/boot/dts/adlink/Makefile
@@ -0,0 +1,7 @@
+# SPDX-License-Identifier: GPL-2.0
+
+
+dtb-$(CONFIG_ARCH_MXC) +=  \
+			  osm-imx93.dtb \
+			  osm-imx93-lvds-panel.dtb
+
