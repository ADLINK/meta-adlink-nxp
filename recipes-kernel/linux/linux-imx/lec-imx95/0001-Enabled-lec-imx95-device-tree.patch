diff --git a/arch/arm64/boot/dts/adlink/Makefile b/arch/arm64/boot/dts/adlink/Makefile
new file mode 100644
index 000000000000..3579c4309367
--- /dev/null
+++ b/arch/arm64/boot/dts/adlink/Makefile
@@ -0,0 +1,5 @@
+dtb-$(CONFIG_ARCH_MXC) += lec-imx95-g080uan02.dtb
+dtb-$(CONFIG_ARCH_MXC) += lec-imx95-g156hat02.dtb
