index a225a9784f4f..472058bb4a87 100644
--- a/arch/arm/mach-imx/imx8m/Kconfig
+++ b/arch/arm/mach-imx/imx8m/Kconfig
@@ -341,8 +341,19 @@ config TARGET_IMX8MP_RSB3720A1_6G
