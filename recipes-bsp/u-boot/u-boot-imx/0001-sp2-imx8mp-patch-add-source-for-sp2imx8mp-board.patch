+		(phys_size_t)2 << 30, /* default */
+		(phys_size_t)4 << 30, /* &dram_timing_lpddr4_4gb, */
+		(phys_size_t)8 << 30, /* &dram_timing_lpddr4_8gb, */
+		(phys_size_t)1 << 30, /* &dram_timing_lpddr4_1gb, */
+		(phys_size_t)2 << 30, /* NULL, default to 2gb */
+		(phys_size_t)2 << 30, /* NULL, default to 2gb */
+		(phys_size_t)2 << 30, /* NULL, default to 2gb */
+		(phys_size_t)2 << 30, /* NULL, default to 2gb */
+	};
+	uint32_t hwid = gd->spl_handoff->arch.sku;
+	if (hwid >= 0 && hwid <= 8) {
+		*size = dram_size[hwid];
+	} else {
+		printf("Unknown DRAM size!!!\n");
+		return -1;
+	}
+	return 0;
