					port@1 {
						reg = <1>;
					};
				};
			};

			ldb0_phy: phy@8 {
				compatible = "fsl,imx95-lvds0-phy";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x8 0x4>;
				gpr = <&lvds_csr>;
				clocks = <&scmi_clk IMX95_CLK_DISPAPB>;
				clock-names = "apb";
				power-domains = <&scmi_devpd IMX95_PD_DISPLAY>;
